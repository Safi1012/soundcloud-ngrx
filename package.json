{
  "name": "soundcloud-ngrx",
  "version": "0.4.0",
  "main": "./src/index.js",
  "description": "SoundCloud API client with Angular2 and NgRx",
  "homepage": "https://github.com/r-park/soundcloud-ngrx",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/soundcloud-ngrx.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "del-cli target",
    "heroku-postbuild": "npm run build",
    "lint": "run-s lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js server",
    "lint:ts": "tslint 'src/**/*.ts'",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "pretest:server": "npm run build",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run server:dev",
    "test": "run-s test:client test:server",
    "test:client": "cross-env NODE_ENV=test karma start --single-run",
    "test:server": "ava server/spec.js --verbose",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "compression": "~1.6.2",
    "express": "~4.14.1",
    "helmet": "~3.4.0",
    "serve-favicon": "~2.3.2",
    "sw-toolbox": "^3.4.0",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@angular/common": "2.4.7",
    "@angular/compiler": "2.4.7",
    "@angular/core": "2.4.7",
    "@angular/forms": "2.4.7",
    "@angular/http": "2.4.7",
    "@angular/platform-browser": "2.4.7",
    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/router": "3.4.7",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@types/jasmine": "~2.5.43",
    "@types/node": "~7.0.5",
    "@types/service_worker_api": "0.0.8",
    "autoprefixer": "~6.7.2",
    "ava": "~0.18.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.0",
    "babel-preset-es2015": "^6.22.0",
    "conventional-changelog-cli": "~1.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "~2.4.1",
    "cross-env": "~3.1.4",
    "css-loader": "~0.26.1",
    "del-cli": "~0.2.1",
    "electron": "^1.4.15",
    "eslint": "~3.15.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "~2.28.0",
    "immutable": "~3.8.1",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.2",
    "minx": "r-park/minx.git",
    "morgan": "~1.8.1",
    "node-sass": "~4.5.0",
    "nodemon": "~1.11.0",
    "npm-run-all": "~4.0.1",
    "postcss-loader": "~1.3.0",
    "purifycss-webpack": "^0.4.2",
    "raw-loader": "~0.5.1",
    "rxjs": "5.1.1",
    "sass-loader": "~6.0.0",
    "serviceworker-webpack-plugin": "^0.2.0",
    "style-ext-html-webpack-plugin": "^3.2.0",
    "style-loader": "~0.13.1",
    "supertest": "~3.0.0",
    "ts-helpers": "~1.1.2",
    "ts-loader": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-md5-hash": "~0.0.5",
    "zone.js": "~0.7.7"
  }
}
